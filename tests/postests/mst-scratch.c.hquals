qualif SIZE(v:ptr)     : ((v + (16 * ~a)) <= BLOCK_END([v]))
qualif BRSZ(v:ptr)     : BLOCK_END([v]) = BLOCK_BEGIN([v]) + 8
qualif VRSZ(v:ptr)     : v != 0 => ((BLOCK_END([v]) - v) >= 16)
qualif HHSZ(v:ptr)     : v != 0 => BLOCK_END([v]) >= v + 12
qualif GTE4(v:int)     : v >= 4

// pmr: Should go away when we add qual inference
qualif ARRAYMOD(v:ptr) : ((v - BLOCK_BEGIN([v])) mod 16) = 0