// Should be derivable
qualif LISTSZ (v : ptr) : (v != 0) -> (BLOCK_END([v]) = (BLOCK_BEGIN([v]) + 12))

// More necessary
qualif CMP    (v : int) : v = ((@data > (DEREF([@tmp_list]) : int)) ? 1 : 0)
qualif NEXT   (v : ptr) : (v != 0) -> (~A <= (DEREF([v]): int))
qualif PREV   (v : ptr) : (v != 0) -> (~A >= (DEREF([v]): int))
