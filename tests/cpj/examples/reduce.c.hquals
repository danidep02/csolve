qualif LEN(v: ptr)        : (BLOCK_END([v]) - BLOCK_BEGIN([v])) >= (@len + @i)
qualif WRITEEFF(v: ptr)   : && [@a + @i <= v; v < (@a + (@i + @len))]
qualif WRITELEFT(v: ptr)  : v <  @a + @i + @hl
qualif WRITERIGHT(v: ptr) : v >= @a + @i + @hl

qualif LEN(v: ptr)    : (BLOCK_END([v]) - BLOCK_BEGIN([v])) >= (2 * @stride)
qualif MOD(v: int)    : (v mod 8) = @i
qualif MOD(v: ptr)    : (EWRITE = 1) => && [((v - @a) mod 8) = @i; v < (@a + @stride)]
qualif MOD(v: ptr)    : (EREAD = 1) => (v >= (@a + @stride))
qualif NOACCM(v: ptr) : EAccumulate != 1