qualif TAGGED(v:ptr, a:int) : TAG([v]) = a
qualif TAGGED2(v:int) : || [TAG([v]) = _; TAG([v]) = _]
qualif TAGGED2(v:ptr) : || [TAG([v]) = _; TAG([v]) = _]
qualif TAGGED2(v:ptr, a:int, b:int) : || [TAG([v]) = v; TAG([v]) = b]
qualif TAGGED2(v:int, a:int, b:int) : || [TAG([v]) = a; TAG([v]) = b]
qualif TAGGED2(v:int, a:int, b:int) : || [TAG([v]) = v; TAG([v]) = b]
qualif TAGGED(v:int)        : TAG([v]) = v
qualif TAG_EQ(v:ptr, p:ptr) : TAG([v]) = TAG([p])
qualif TAG_EQ(v:ptr, a:int) : TAG([v]) = TAG([a])