qualif DPAIR(v:ptr)    : (v + 16) = BLOCK_END([v])
qualif BSIZEDEP(v:ptr) : (v != 0) -> (BLOCK_END([v]) = (BLOCK_BEGIN([v]) + 92))
qualif RSIZEDEP(v:ptr) : BLOCK_END([v]) = (BLOCK_BEGIN([v]) + 104)
qualif LSIZEDEP(v:ptr) : (v != 0) -> (BLOCK_END([v]) = (BLOCK_BEGIN([v]) + 56))
qualif LT10(v:int)     : v <= 10
qualif BRINLAT(v:ptr)  : (@num > 0) -> (0 < v)
